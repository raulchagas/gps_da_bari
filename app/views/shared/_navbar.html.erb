<div class="container-fluid style: margin-top 10px;">
<%# <%acho que esse styling nÃ£o vai funcionar. tem que fechar aspas da classe e colocar o style. ou usar bootstrap mt-10%>

  <div class="navbar navbar-expand-sm navbar-light navbar-lewagon nav-bari">
    <div class="container-fluid">
      <%= link_to "#", class: "navbar-brand" do %>
        <%= image_tag "user.png" %>
      <% end %>
      <h1 class="font-white">Hello user</h1>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="fa-solid fa-bars icon-white"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto dropdown-color text-center">
          <% if user_signed_in? %>
            <li class="nav-item active icon-white
            ">
              <%= link_to "Home", dashboards_path, class: "nav-link  icon-white" %>
            </li>
            <li class="nav-item">
              <%= link_to "Messages", "#", class: "nav-link  icon-white" %>
            </li>
            <li class="nav-item">
              <%= link_to "Edit my profile", edit_user_registration_path, class: "nav-link  icon-white"%>
            </li>
            <li class="nav-item">
              <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "nav-link  icon-white"%>
            </li>
          <% else %>
            <li class="nav-item">
              <li><%= link_to "Sign Up", new_user_registration_path, class: "nav-link icon-white" %></li>
              <li><%= link_to "Login", new_user_session_path, class: "nav-link icon-white" %></li>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
